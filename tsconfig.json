{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "declarations.d.ts",
    "src/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/onboarding/onboard.tsx",
    "components/onboarding/onboard.tsx",
    "components/onboarding/onboard.tsx"
  ]
}
